{
  "image_view": {
    "nr_loaded_images": 6,
    "should_wait": true,
    "frame_size_relative_to_image": 0.2,
    "scroll_navigation": true,
    "name_format": "$(#File Name#)$( • ƒ#Aperture#)$( • #Shutter Speed#)$( • #ISO# ISO)",
    "user_actions": [ 
      {
        "shortcut": {
          "key": "c",
          "modifiers": [ "shift" ] 
        },
        "exec": "bash -c 'magick {} png:- | wl-copy && notify-send -i {} \"Copied Image to Clipboard!\" -t 5000'",
        "callback": null
      },
      {
        "shortcut": {
          "key": "delete",
          "modifiers": [  ] 
          },
          "exec": "bash -c 'mkdir -p $HOME/trash && mv -f {} $HOME/trash && notify-send -i $HOME/trash/{/} \"Moved Image to Trash!\" -t 5000 -u critical'",
          "callback": "Pop"
      }
    ],
    "context_menu": [
      {
        "description": "Copy to Clipboard",
        "exec": "bash -c 'magick {} png:- | wl-copy && notify-send -i {} \"Copied Image to Clipboard!\" -t 5000'",
        "callback": null
      },
      {
        "description": "Delete",
        "exec": "bash -c 'mkdir -p $HOME/trash && mv -f {} $HOME/trash && notify-send -i $HOME/trash/{/} \"Moved Image to Trash!\" -t 5000 -u critical'",
        "callback": "Pop"
      }
    ],
    "sc_fit": {
      "key": "f",
      "modifiers": []
    },
    "sc_frame": {
      "key": "g",
      "modifiers": []
    },
    "sc_zoom": {
      "key": "Space",
      "modifiers": []
    },
    "sc_next": {
      "key": "ArrowRight",
      "modifiers": []
    },
