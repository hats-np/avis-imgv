      "modifiers": []
    },
    "sc_less_per_row": {
      "key": "Minus",
      "modifiers": []
    }
  },
  "general": {
    "limit_cached": 1000000,
    "output_icc_profile": "srgb",
    "text_scaling": 1.25,
